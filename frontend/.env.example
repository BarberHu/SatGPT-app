# React App Environment Variables
# Copy this file to .env and fill in the values

# Mapbox Access Key (required for map functionality)
REACT_APP_MAPBOX_ACCESS_KEY=your_mapbox_access_key_here

# API URL (optional - defaults to empty for same-origin requests)
# In development, the proxy in package.json handles routing to Flask backend
# In production, set this to your Flask backend URL
REACT_APP_API_URL=
